<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.edu.ynu.ordinarydraw.domain">

	<class name="Subscribe" table="t_subscribe" lazy="true">
		<composite-id>
			<key-property name="userid" type="int" column="user_id"
				length="5"></key-property>
			<key-property name="tagid" type="int" column="tag_id"
				length="5"></key-property>
		</composite-id>
		<property name="subdate" type="string" column="sub_date"></property>

	</class>

</hibernate-mapping>